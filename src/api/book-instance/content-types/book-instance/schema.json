{
  "kind": "collectionType",
  "collectionName": "book_instances",
  "info": {
    "singularName": "book-instance",
    "pluralName": "book-instances",
    "displayName": "Book-instance"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "book": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::book.book",
      "inversedBy": "book_instances"
    },
    "imprint": {
      "type": "string"
    },
    "status": {
      "type": "enumeration",
      "enum": [
        "Maintenance",
        "Available",
        "Loaned",
        "Reserved"
      ],
      "default": "Maintenance",
      "required": true
    },
    "due_back": {
      "type": "datetime"
    }
  }
}
