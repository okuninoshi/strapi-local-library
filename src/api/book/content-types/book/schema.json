{
  "kind": "collectionType",
  "collectionName": "books",
  "info": {
    "singularName": "book",
    "pluralName": "books",
    "displayName": "Book",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "summary": {
      "type": "text"
    },
    "author": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::author.author",
      "inversedBy": "books"
    },
    "genres": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::genre.genre",
      "inversedBy": "books"
    },
    "ISBN": {
      "type": "string",
      "unique": true,
      "required": true,
      "maxLength": 13,
      "minLength": 10
    },
    "book_instances": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::book-instance.book-instance",
      "mappedBy": "book"
    }
  }
}
